#!/bin/sh
#
# A bucket for all incomming files

echo "Processing Bucket..."
# XDF files to current experiment
for filename in $(ls ~/.bkt | grep "xdf");
do
  cp -u "$HOME/.bkt/$filename" "/home/beto/cloud/Research/Abstract/out/"
  cd /home/beto/cloud/Research/Abstract/
  python src/preprocessing.py
done

echo "Done"
