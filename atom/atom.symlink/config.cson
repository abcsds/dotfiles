"*":
  editor:
    showIndentGuide: true
    invisibles: {}
    softWrapAtPreferredLineLength: true
    showInvisibles: true
    fontSize: 17
    scrollPastEnd: true
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
    hideGitIgnoredFiles: false
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "figlet"
    ]
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  welcome:
    showOnStartup: false
  "git-plus": {}
  "one-dark-ui": {}
  "markdown-preview-plus": {}
  emmet: {}
  tidal:
    ghciPath: "/usr/local/bin/ghc"
  "git-log": {}
  figlet: {}
  pigments:
    extendAutocompleteToVariables: true
  blame: {}
  "atom-css-comb":
    projectConfigs: "~/.csscomb.json"
  linter: {}
